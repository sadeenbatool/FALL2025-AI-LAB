    "        print(\"FizzBuzz\")\n",
    "    elif y%3==0:\n",
    "        print(\"Fizz\")\n",
    "    elif y%5==0:\n",
    "        print(\"Buzz\")\n",
    "    else:\n",
    "        print(y)"
